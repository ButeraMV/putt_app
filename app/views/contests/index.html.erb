<div class="container text-center">
  <%= link_to 'New Contest', new_contest_path, class: 'btn btn-primary new-contest-button' %>
  <table class="table table-striped contest-table">
    <% @contests.each do |contest| %>
      <tr>
        <td>
          <%= contest.name %>
        </td>
      </tr>
    <% end %>
  </table>
</div>



